            "settings": {
              "image": "factoryairegistry.azurecr.io/intelligentedge/webmodule:0.35.0-amd64",
              "createOptions": "{\"HostConfig\":{\"Mounts\":[{\"Target\":\"/workspace\",\"Source\":\"ovmsworkspace\",\"Type\":\"volume\"}],\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"PortBindings\":{\"8000/tcp\":[{\"HostPort\":\"8000\"}]}}}"
            }
          },
