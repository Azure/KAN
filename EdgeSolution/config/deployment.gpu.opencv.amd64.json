              "IOTHUB_CONNECTION_STRING": {
                "value": "HostName=customvision.azure-devices.net;SharedAccessKeyName=iothubowner;SharedAccessKey=DobFRt0WwiJz/xOnGZKQcJ/yRgEEoPEvHPJNfO2Yj0s="
              },
              "LVA_MODE": {
                "value": "grpc"
              }
            },
            "settings": {
              "image": "factoryairegistry.azurecr.io/intelligentedge/predictmodule:0.35.5-gpuamd64",
              "createOptions": "{\"HostConfig\":{\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"PortBindings\":{\"7777/tcp\":[{\"HostPort\":\"7777\"}]},\"IpcMode\":\"host\",\"runtime\":\"nvidia\"}}"
            }
          },
          "cvcapturemodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
