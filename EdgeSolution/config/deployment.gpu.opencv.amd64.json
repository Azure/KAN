                "value": "grpc"
              },
              "IS_OPENCV": {
                "value": "true"
              }
            },
            "settings": {
              "image": "factoryairegistry.azurecr.io/intelligentedge/inferencemodule:0.35.0-amd64",
              "createOptions": "{\"HostConfig\":{\"LogConfig\":{\"Type\":\"\",\"Config\":{\"max-size\":\"10m\",\"max-file\":\"10\"}},\"PortBindings\":{\"5000/tcp\":[{\"HostPort\":\"5000\"}],\"5558/tcp\":[{\"HostPort\":\"5558\"}]},\"runtime\":\"nvidia\"}}"
            }
          },
          "predictmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
